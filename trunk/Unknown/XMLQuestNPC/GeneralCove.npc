    <Pause>1</Pause>
    <PrePause>-1</PrePause>
    <LockConversation>False</LockConversation>
    <IgnoreCarried>False</IgnoreCarried>
    <AllowNPCTrigger>False</AllowNPCTrigger>
    <SpeechStyle>Regular</SpeechStyle>
    <SpeechHue>-1</SpeechHue>
  </SpeechEntry>
  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>Far South of here there are Goats attacking please kill 40 of them.</Text>
    <Keywords>yes,ok,sure,accept</Keywords>
    <Action>SETONTRIGMOB/ATTACH/XmlData,CoveQuest1; GIVE/&lt;questholder/name/CoveQuest1/notestring/40 Goat/objective1/KILL,Goat,40/autoreward/true/repeatable/false/rewardstring/@bag/ADD/bankcheck,2000</Action>
